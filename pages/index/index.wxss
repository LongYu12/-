/* components/SearchInput/SearchInput.wxss */
.swiper_i {
  height: 340rpx;
  padding: 0 20rpx 0 20rpx;
  background-color: var(--themeColor);
}
.swiper_i image {
  width: 100%;
}
.index_cate {
  display: flex;
  background-color: var(--themeColor);
}
.index_cate navigator {
  flex: 1;
  padding: 0 30rpx 10rpx 30rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor {
  padding: 0 15rpx;
  background-color: var(--themeColor);
}
.index_floor .index_img {
  padding-top: 15rpx;
  background-color: rgba(236, 104, 236, 0.952);
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.index_floor .index_img image {
  width: 100%;
}
.index_floor .index_content {
  overflow: hidden;
}
.index_floor .index_content navigator {
  float: left;
  width: 33.3%;
  border-top: 10rpx;
}
.index_floor .index_content navigator image {
  width: 100%;
  height: 100%;
}
.index_floor .index_content navigator:nth-child(n + 2) image {
  /* 
border-left:10rpx solid var(--themeColor);原图的宽高232*386232/386=33.33vw/height第一张照片的高度height:33.33vw * 386 / 232 / 2
          */
  height: 26.20474138vw;
}
.index_floor .index_content navigator:nth-child(4),
.index_floor .index_content navigator:nth-child(5) {
  border-bottom: 10rpx;
  border-top: 10rpx;
}
image{
  width: 100%;
}